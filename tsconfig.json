{
    "compilerOptions": {
        "target": "es6",
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "outDir": "./dist/",
        "rootDir": "./src/",
        "types": [
            "p5"
        ],
        "typeRoots": [
            "node_modules/@types"
        ],
        "noImplicitThis": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true
    }
}